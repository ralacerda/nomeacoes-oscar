---
import ImdbLink from "./ImdbLink.astro";

export interface Props {
  castArray: { name: string; nameID: string }[];
}

const { castArray } = Astro.props;
---

{
  castArray.map((castMember) =>
    castMember.name != "N/A" ? (
      <ImdbLink id={castMember.nameID} type="name">
        {castMember.name}
      </ImdbLink>
    ) : (
      ""
    )
  )
}
